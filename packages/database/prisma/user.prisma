generator client {
  provider = "prisma-client-js"
  output   = "../generated/user-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_USER")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
